<!DOCTYPE template>
<Template>

    <!--
    **************************************************************************************
    BRAND DEVICE BEGIN
    **************************************************************************************
    -->
    <WidgetGroup>
        <ObjectName>Brand</ObjectName>
        <Layout>vertical</Layout>
        <Size>150f,69f</Size>
        <Children>

            <Template src="skin:device/brand/border_top.xml"/>

            <WidgetGroup>
                <ObjectName>Rectangle</ObjectName>
                <Layout>horizontal</Layout>
                <Size>150f,45f</Size>
                <Children>

                    <WidgetGroup><!-- spacer -->
                        <MinimumSize>10,-1</MinimumSize>
                        <MaximumSize>10,-1</MaximumSize>
                    </WidgetGroup>

                    <WidgetGroup>
                        <ObjectName>Logo</ObjectName>
                        <Size>130f,45f</Size>
                        <Children>

                            <Label>
                                <Size>130f,45f</Size>
                                <ObjectName>Version</ObjectName>
                                <Text>v.2.1.0</Text>
                                <Alignment>right</Alignment>
                            </Label>

                        </Children>
                    </WidgetGroup>

                    <WidgetGroup><!-- spacer -->
                        <MinimumSize>10,-1</MinimumSize>
                        <MaximumSize>10,-1</MaximumSize>
                    </WidgetGroup>

                </Children>
            </WidgetGroup>

            <Template src="skin:device/brand/border_bottom.xml"/>

        </Children>
    </WidgetGroup>

    <WidgetGroup><!-- margin-right-->
        <MinimumSize>4,-1</MinimumSize>
        <MaximumSize>4,-1</MaximumSize>
    </WidgetGroup>
    <!--
    **************************************************************************************
    BRAND DEVICE END
    **************************************************************************************
    -->

</Template>