<!DOCTYPE skin>
<skin>

    <manifest>
        <title>RoundCorners</title>
        <author>22degrees</author>
        <version>2.1.0.01</version>
        <description>A 4-deck full-featured skin</description>
        <language>en</language>
        <license>Creative Commons Attribution, Share-Alike 3.0 Unported</license>
        <attributes>

            <!-- Main -->
            <attribute persist="true" config_key="[Main],show_main">1</attribute>
            <!-- Internal Clock -->
            <attribute persist="true" config_key="[InternalClock],show_internalclock">1</attribute>
            <!-- Headphone -->
            <attribute persist="true" config_key="[Headphone],show_headphone">0</attribute>
            <!-- Clock-->
            <attribute persist="true" config_key="[Clock],show_clock">0</attribute>
            <!-- Stacked Waveforms -->
            <attribute persist="true" config_key="[Visuals],show_row">0</attribute>
            <!-- Deck 1-4 -->
            <attribute persist="true" config_key="[Master],num_decks">4</attribute>
            <!-- Deck 1 -->
            <attribute persist="true" config_key="[DeckRow1],show_row">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_spinny">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_pitch">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_bpm">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_transport">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_hotcue">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_advanced_markers">1</attribute>
            <attribute persist="true" config_key="[Deck1],show_loop">1</attribute>
            <!-- Mixer 1 -->
            <attribute persist="true" config_key="[Mixer1],show_mixer">1</attribute>
            <attribute persist="true" config_key="[Mixer1],show_filter">1</attribute>
            <attribute persist="true" config_key="[Mixer1],show_eq">1</attribute>
            <attribute persist="true" config_key="[Mixer1],show_fader">1</attribute>
            <attribute persist="true" config_key="[Mixer1],show_vu">1</attribute>
            <!-- Deck 2 -->
            <attribute persist="true" config_key="[Deck2],show_spinny">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_pitch">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_bpm">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_transport">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_hotcue">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_advanced_markers">1</attribute>
            <attribute persist="true" config_key="[Deck2],show_loop">1</attribute>
            <!-- Deck 3 -->
            <attribute persist="true" config_key="[DeckRow2],show_row">0</attribute>
            <attribute persist="true" config_key="[Deck3],show_spinny">0</attribute>
            <attribute persist="true" config_key="[Deck3],show_pitch">0</attribute>
            <attribute persist="true" config_key="[Deck3],show_bpm">1</attribute>
            <attribute persist="true" config_key="[Deck3],show_transport">1</attribute>
            <attribute persist="true" config_key="[Deck3],show_hotcue">0</attribute>
            <attribute persist="true" config_key="[Deck3],show_advanced_markers">1</attribute>
            <attribute persist="true" config_key="[Deck3],show_loop">0</attribute>
            <!-- Mixer 2 -->
            <attribute persist="true" config_key="[Mixer2],show_mixer">1</attribute>
            <attribute persist="true" config_key="[Mixer2],show_filter">1</attribute>
            <attribute persist="true" config_key="[Mixer2],show_eq">1</attribute>
            <attribute persist="true" config_key="[Mixer2],show_fader">1</attribute>
            <attribute persist="true" config_key="[Mixer2],show_vu">1</attribute>
            <!-- Deck 4 -->
            <attribute persist="true" config_key="[Deck4],show_spinny">0</attribute>
            <attribute persist="true" config_key="[Deck4],show_pitch">0</attribute>
            <attribute persist="true" config_key="[Deck4],show_bpm">1</attribute>
            <attribute persist="true" config_key="[Deck4],show_transport">1</attribute>
            <attribute persist="true" config_key="[Deck4],show_hotcue">0</attribute>
            <attribute persist="true" config_key="[Deck4],show_advanced_markers">1</attribute>
            <attribute persist="true" config_key="[Deck4],show_loop">0</attribute>
            <!-- VinylControl/Crossfader -->
            <attribute persist="true" config_key="[VinylControl],show_vinylcontrol">0</attribute>
            <!-- Microphone/Aux-->
            <attribute persist="true" config_key="[Master],num_microphones">2</attribute>
            <attribute persist="true" config_key="[Master],num_auxiliaries">2</attribute>
            <attribute persist="true" config_key="[Microphone],show_microphone">0</attribute>
            <!-- Samplers -->
            <attribute persist="true" config_key="[Master],num_samplers">4</attribute>
            <attribute persist="true" config_key="[Samplers],show_samplers">1</attribute>
            <attribute persist="true" config_key="[Samplers],show_vumeter">0</attribute>
            <attribute persist="true" config_key="[Samplers],show_transport">1</attribute>
            <attribute persist="true" config_key="[Samplers],show_advanced_markers">1</attribute>
            <!-- Effects -->
            <attribute persist="true" config_key="[Master],num_effectunits">4</attribute>
            <attribute persist="true" config_key="[EffectRack1],show">0</attribute>
            <attribute persist="true" config_key="[EffectUnit],show_controls">1</attribute>
            <attribute persist="true" config_key="[EffectUnit],show_chain">0</attribute>
            <!-- Library/Coverart -->
            <attribute persist="true" config_key="[Library],show_minimized">1</attribute>
            <attribute persist="true" config_key="[Library],show_coverart">0</attribute>
            <!-- Previewdeck -->
            <attribute persist="true" config_key="[Master],num_preview_decks">1</attribute>
            <attribute persist="true" config_key="[PreviewDeck],show_previewdeck">0</attribute>

        </attributes>
    </manifest>

    <ObjectName>Body</ObjectName>
    <Style src="skin:style.qss"/>
    <MinimumSize>1024,-1</MinimumSize>
    <SizePolicy>me,me</SizePolicy>
    <Layout>vertical</Layout>
    <Children>

        <Template src="skin:unit/menubar.xml"/>

        <WidgetGroup>
            <SizePolicy>me,max</SizePolicy>
            <Layout>vertical</Layout>
            <Children>

                <Template src="skin:unit/waveforms.xml">
                    <SetVariable name="Number1">1</SetVariable>
                    <SetVariable name="Widget1">[Deck1]</SetVariable>
                    <SetVariable name="Group1">[Channel1]</SetVariable>
                    <SetVariable name="Align1">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor1">#d4ddd7</SetVariable>
                    <SetVariable name="SignalMidColor1">#a4aba6</SetVariable>
                    <SetVariable name="SignalLowColor1">#737875</SetVariable>
                    <SetVariable name="MarkRangeColor1">#417587</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor1">#979a9c</SetVariable>

                    <SetVariable name="Number2">2</SetVariable>
                    <SetVariable name="Widget2">[Deck2]</SetVariable>
                    <SetVariable name="Group2">[Channel2]</SetVariable>
                    <SetVariable name="Align2">top</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor2">#9ab0b5</SetVariable>
                    <SetVariable name="SignalMidColor2">#7c9297</SetVariable>
                    <SetVariable name="SignalLowColor2">#5e7479</SetVariable>
                    <SetVariable name="MarkRangeColor2">#36457d</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor2">#828590</SetVariable>

                    <SetVariable name="Number3">3</SetVariable>
                    <SetVariable name="Widget3">[Deck3]</SetVariable>
                    <SetVariable name="Group3">[Channel3]</SetVariable>
                    <SetVariable name="Align3">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor3">#918b74</SetVariable>
                    <SetVariable name="SignalMidColor3">#736d56</SetVariable>
                    <SetVariable name="SignalLowColor3">#554f38</SetVariable>
                    <SetVariable name="MarkRangeColor3">#5e5c26</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor3">#6d6c5c</SetVariable>

                    <SetVariable name="Number4">4</SetVariable>
                    <SetVariable name="Widget4">[Deck4]</SetVariable>
                    <SetVariable name="Group4">[Channel4]</SetVariable>
                    <SetVariable name="Align4">top</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor4">#776a54</SetVariable>
                    <SetVariable name="SignalMidColor4">#594c36</SetVariable>
                    <SetVariable name="SignalLowColor4">#3b2e18</SetVariable>
                    <SetVariable name="MarkRangeColor4">#4d431b</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor4">#595546</SetVariable>
                </Template>

                <Template src="skin:unit/decks.xml">
                    <SetVariable name="Number1">1</SetVariable>
                    <SetVariable name="Widget1">[Deck1]</SetVariable>
                    <SetVariable name="Group1">[Channel1]</SetVariable>
                    <SetVariable name="Align1">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor1">#d4ddd7</SetVariable>
                    <SetVariable name="SignalMidColor1">#a4aba6</SetVariable>
                    <SetVariable name="SignalLowColor1">#737875</SetVariable>
                    <SetVariable name="MarkRangeColor1">#417587</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor1">#979a9c</SetVariable>

                    <SetVariable name="Number2">2</SetVariable>
                    <SetVariable name="Widget2">[Deck2]</SetVariable>
                    <SetVariable name="Group2">[Channel2]</SetVariable>
                    <SetVariable name="Align2">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor2">#9ab0b5</SetVariable>
                    <SetVariable name="SignalMidColor2">#7c9297</SetVariable>
                    <SetVariable name="SignalLowColor2">#5e7479</SetVariable>
                    <SetVariable name="MarkRangeColor2">#36457d</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor2">#828590</SetVariable>

                    <SetVariable name="Number3">1</SetVariable>
                    <SetVariable name="Widget3">[Mixer1]</SetVariable>

                    <SetVariable name="Widget4">[DeckRow1]</SetVariable>
                </Template>

                <Template src="skin:unit/decks.xml">
                    <SetVariable name="Number1">3</SetVariable>
                    <SetVariable name="Widget1">[Deck3]</SetVariable>
                    <SetVariable name="Group1">[Channel3]</SetVariable>
                    <SetVariable name="Align1">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor1">#918b74</SetVariable>
                    <SetVariable name="SignalMidColor1">#736d56</SetVariable>
                    <SetVariable name="SignalLowColor1">#554f38</SetVariable>
                    <SetVariable name="MarkRangeColor1">#5e5c26</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor1">#6d6c5c</SetVariable>

                    <SetVariable name="Number2">4</SetVariable>
                    <SetVariable name="Widget2">[Deck4]</SetVariable>
                    <SetVariable name="Group2">[Channel4]</SetVariable>
                    <SetVariable name="Align2">bottom</SetVariable> <!-- Values: top/center/bottom -->
                    <SetVariable name="SignalHighColor2">#776a54</SetVariable>
                    <SetVariable name="SignalMidColor2">#594c36</SetVariable>
                    <SetVariable name="SignalLowColor2">#3b2e18</SetVariable>
                    <SetVariable name="MarkRangeColor2">#4d431b</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor2">#595546</SetVariable>

                    <SetVariable name="Number3">2</SetVariable>
                    <SetVariable name="Widget3">[Mixer2]</SetVariable>

                    <SetVariable name="Widget4">[DeckRow2]</SetVariable>
                </Template>

                <Template src="skin:unit/vinylcontrols.xml"/>

                <Template src="skin:unit/microphone/index.xml"/>

                <Template src="skin:unit/sampler/index.xml">
                    <SetVariable name="Number1">1</SetVariable>
                    <SetVariable name="Widget1">[Sampler1]</SetVariable>
                    <SetVariable name="Group1">[Sampler1]</SetVariable>
                    <SetVariable name="SignalHighColor1">#d4ddd7</SetVariable>
                    <SetVariable name="SignalMidColor1">#a4aba6</SetVariable>
                    <SetVariable name="SignalLowColor1">#737875</SetVariable>
                    <SetVariable name="MarkRangeColor1">#417587</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor1">#979a9c</SetVariable>

                    <SetVariable name="Number2">2</SetVariable>
                    <SetVariable name="Widget2">[Sampler2]</SetVariable>
                    <SetVariable name="Group2">[Sampler2]</SetVariable>
                    <SetVariable name="SignalHighColor2">#9ab0b5</SetVariable>
                    <SetVariable name="SignalMidColor2">#7c9297</SetVariable>
                    <SetVariable name="SignalLowColor2">#5e7479</SetVariable>
                    <SetVariable name="MarkRangeColor2">#36457d</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor2">#828590</SetVariable>

                    <SetVariable name="Number3">3</SetVariable>
                    <SetVariable name="Widget3">[Sampler3]</SetVariable>
                    <SetVariable name="Group3">[Sampler3]</SetVariable>
                    <SetVariable name="SignalHighColor3">#918b74</SetVariable>
                    <SetVariable name="SignalMidColor3">#736d56</SetVariable>
                    <SetVariable name="SignalLowColor3">#554f38</SetVariable>
                    <SetVariable name="MarkRangeColor3">#5e5c26</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor3">#6d6c5c</SetVariable>

                    <SetVariable name="Number4">4</SetVariable>
                    <SetVariable name="Widget4">[Sampler4]</SetVariable>
                    <SetVariable name="Group4">[Sampler4]</SetVariable>
                    <SetVariable name="SignalHighColor4">#776a54</SetVariable>
                    <SetVariable name="SignalMidColor4">#594c36</SetVariable>
                    <SetVariable name="SignalLowColor4">#3b2e18</SetVariable>
                    <SetVariable name="MarkRangeColor4">#4d431b</SetVariable>
                    <SetVariable name="MarkRangeDisabledColor4">#595546</SetVariable>
                </Template>

                <Template src="skin:unit/effect/index.xml">
                    <SetVariable name="EffectRackNumber">1</SetVariable>
                </Template>

            </Children>
            <Connection>
                <ConfigKey persist="true">[Master],maximize_library</ConfigKey>
                <BindProperty>visible</BindProperty>
                <Transform>
                    <Not/>
                </Transform>
            </Connection>
        </WidgetGroup>

        <Template src="skin:unit/library.xml"/>

    </Children>
</skin>