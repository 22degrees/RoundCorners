/* SIDEBAR
   ========================================================================== */

/**
 * Default Styles/Resets
 */
#Sidebar {
    border: 0;
    border-radius: 0;
    background: #121212;
    margin: 0;
    padding: 0;
    height: 0;
    width: 0;
}

QTreeView {
    border: 0;
    show-decoration-selected: 0;
    outline: none;
    background: transparent;
    font-size: 13px;
    color: #c6c6c6;
}

/**
 * TreeView-Items
 */
QTreeView::item {
    border: 0;
    width: 400px;
    height: 20px;
    padding-left: 5px;
}

QTreeView::item:hover {
    background: transparent;
}

QTreeView::item:selected:active {
    background-color: #98a19b;
    color: #000000;
}

QTreeView::item:selected:!active {
    background-color: #98a19b;
    color: #000000;
}

/**
 * TreeView-Branches
 */
QTreeView::branch:selected:active {
    background-color: #98a19b;
}

QTreeView::branch:selected:!active {
    background-color: #98a19b;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(skin:/style/branch_closed.svg);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings {
    border-image: none;
    image: url(skin:/style/branch_open.svg);
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(skin:/style/branch_line.svg) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(skin:/style/branch_more.svg) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
    border-image: url(skin:/style/branch_end.svg) 0;
}

/* SPLITTER
   ========================================================================== */

QSplitter {
    width: 4px;
    margin: 0;
    padding: 0;
    border: 0;
}

QSplitter::handle {
    image: url(skin:/style/handle_unchecked.svg);
    background: none;
    width: 4px;
    margin: 0;
    padding: 0;
}

QSplitter::handle:vertical {
    background: none;
    margin: 0;
    padding: 0;
    width: 0;
}

QSplitter::handle:pressed {
    image: url(skin:/style/handle_checked.svg);
    background: none;
}

