#Slider {
    background: transparent url(skin:/background/bg_1_slider.svg) repeat-y center top;
}

#SliderTop {
    background: transparent url(skin:/background/bg_1_slider_top.svg) no-repeat center top;
}

#SliderBottom {
    background: transparent url(skin:/background/bg_1_slider_bottom.svg) no-repeat center top;
}

#PitchSlider {
    background: transparent url(skin:/background/bg_2_slider.svg) repeat-y center top;
}

#PitchSliderTop {
    background: transparent url(skin:/background/bg_2_slider_top.svg) no-repeat center top;
}

#PitchSliderBottom {
    background: transparent url(skin:/background/bg_2_slider_bottom.svg) no-repeat center top;
}

#CrossFader {
    background: transparent url(skin:/background/bg_3_slider.svg) no-repeat center top;
}

#CrossFaderLeft {
    background: transparent url(skin:/background/bg_3_slider_left.svg) no-repeat center top;
}

#CrossFaderRight {
    background: transparent url(skin:/background/bg_3_slider_right.svg) no-repeat center top;
}

#Vu {
    background: transparent url(skin:/background/bg_1_vu.svg) repeat-y center top;
}

#VuBottom {
    background: transparent url(skin:/background/bg_1_vu_bottom.svg) no-repeat center top;
}

#MasterVu, #ChannelVu {
    background: transparent url(skin:/background/bg_4_vu.svg) repeat-x right center;
}

#MasterVuBottom, #ChannelVuBottom {
    background: transparent url(skin:/background/bg_4_vu_bottom.svg) no-repeat left center;
}


#TransportRow,
#HotCueRow,
#LoopRow,
#BeatGridRow {
    border: 0;
    border-radius: 0;
    background-color: #121212;
    margin: 0;
    padding: 0;
}
