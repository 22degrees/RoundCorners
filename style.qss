/**
 * Minor Comment Block
 */

WWidgetGroup { border: 0; border-radius: 0; margin: 0; padding: 0; background: transparent; color: #333333; }

/* CONTEXT MENU
   ========================================================================== */

QMenu {
    background-color: #121212;
    border: 1px solid #000000;
    font-size: 14px;
    color: #333333;
}

QMenu::item {
    padding: 2px 20px;
}

QMenu::item:enabled {
    color: #c6c6c6;
}

QMenu::item:enabled:selected {
    background-color: #98a19b;
    color: #000000;
}

QMenu::separator {
    height: 1px;
    background-color: #484848;
    margin: 5px 10px;
}

/* TOOLTIP
   ========================================================================== */

QToolTip {
    background-color: #121212;
    border: 1px solid #000000;
    font-size: 14px;
    color: #c6c6c6;
}

#Body { background-color: #000000; font-family: 'Open Sans'; font-weight: normal; }
#BorderTop { border: 0; border-top-right-radius: 8px; border-top-left-radius: 8px; background-color: #121212; margin: 2px 0 0 0; }
#BorderBottom { border: 0; border-bottom-right-radius: 8px; border-bottom-left-radius: 8px; background-color: #121212; margin: 0 0 2px 0; }
#Rectangle { border: 0; border-radius: 0; background-color: #121212; margin: 0; padding: 0; height: 0; width: 0; }
#RoundedRectangle { border: 0; border-radius: 8px; background-color: #121212; margin: 2px; padding: 0; height: 0; width: 0; }

/* DEVICE-SETTINGS-BOX
   ========================================================================== */

#OuterConfigBox {
    border: 0;
    border-radius: 8px;
    background-color: rgba(0, 0, 0, 180);
    margin: 2px 0 2px 0;
    padding: 0;
    height: 0;
    width: 0;
}

#InnerConfigBox {
    border: 0;
    border-radius: 8px;
    background-color: rgba(25, 25, 25, 210);
    margin: 0;
    padding: 0;
    height: 0;
    width: 0;
}

/* PASSTHROUGH-BOX
   ========================================================================== */

#OuterPassthroughBox {
    border: 0;
    border-radius: 8px;
    background-color: #121212;
    margin: 2px 0 2px 0;
    padding: 0;
    height: 0;
    width: 0;
}

/* SEARCHBAR
   ========================================================================== */

WSearchLineEdit {
    min-height: 32px;
